#include<stdio.h>
int main(){
    int a[50],i,n,ele,frequency=0;
    printf("enter size of array:");
    scanf("%d",&n);
    printf("enter array elements:");
    for(i=0;i<n;i++){
        scanf("%d",&a[i]);
    }
    printf("enter element to find frequency:");
    scanf("%d",&ele);
    for(i=0;i<n;i++){
        if(ele==a[i]){
            frequency++;
        }
    }
    printf("the frequency of %d is %d",ele,frequency);
    return 0;
}
